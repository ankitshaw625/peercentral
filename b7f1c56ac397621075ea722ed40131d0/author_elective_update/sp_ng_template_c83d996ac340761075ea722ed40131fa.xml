        <template><![CDATA[<div class="panel panel-default list-collector-modal-panel">
    <div class="panel-heading">
        <h4 class="panel-title">${Select Options}</h4>
    </div>
    <div class="panel-body">
        <div class="row" style="height: 100%;">
            <div class="col-xs-5">
                <div class="list-collector-column">
                    <label for="available_search">${Available}</label>
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-search"></i></span>
                        <input id="available_search" type="text" class="form-control" placeholder="Search available..." ng-model="search.available">
                    </div>
                    <div class="list-collector-box">
                        <a href="javascript:void(0)"
                           ng-repeat="item in availableChoices | filter:search.available"
                           ng-click="toggleSelection(item, 'available')"
                           ng-dblclick="moveItem(item, 'available')"
                           class="list-group-item"
                           ng-class="{'active': item.selected}"
                           tabindex="0">
                            {{::item.label}}
                        </a>
                    </div>
                </div>
            </div>

            <div class="col-xs-2">
                <div class="list-collector-controls">
                    <button class="btn btn-default btn-block" ng-click="moveSelectedItems('available')" title="Add selected to right" aria-label="Add selected items">
                        <i class="fa fa-chevron-right"></i>
                    </button>
                    <button class="btn btn-default btn-block" ng-click="moveSelectedItems('selected')" title="Remove selected from right" aria-label="Remove selected items">
                        <i class="fa fa-chevron-left"></i>
                    </button>
                </div>
            </div>

            <div class="col-xs-5">
                <div class="list-collector-column">
                    <label for="selected_search">${Selected}</label>
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-search"></i></span>
                        <input id="selected_search" type="text" class="form-control" placeholder="Search selected..." ng-model="search.selected">
                    </div>
                    <div class="list-collector-box">
                        <div ng-if="selectedChoices.length === 0" class="none-selected-text">
                            -- None --
                        </div>
                        <a href="javascript:void(0)"
                           ng-repeat="item in selectedChoices | filter:search.selected"
                           ng-click="toggleSelection(item, 'selected')"
                           ng-dblclick="moveItem(item, 'selected')"
                           class="list-group-item"
                           ng-class="{'active': item.selected}"
                           tabindex="0">
                             {{::item.label}}
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel-footer text-right">
        <button class="btn btn-default" ng-click="cancel()">${Cancel}</button>
        <button class="btn btn-primary" ng-click="apply()">${Apply}</button>
    </div>
</div>]]></template>
    </sp_ng_template>
